AND M.COD_CIA = D.COD_CIA
AND M.COD_DP = D.COD_DP
AND D.COD_CIA  = C.COD_CIA
AND D.COD_CAT = C.COD_CAT
AND M.COD_EMP = $P{COD_EMP}
